<h1>{{this.Position}}</h1>
<form method="post" action="/position/{{this.Position}}/2016">
	<input id="sixInput" type="hidden" name="year" value="2016">
	<button id="sixBtn">2016</button>
</form>

<form method="post" action="/position/{{this.Position}}/2015">
	<input id="fiveInput" type="hidden" name="year" value="2015">
	<button id="fiveBtn">2015</button>
</form>

<form method="post" action="/position/Quarterback/2014">
	<input id="fourInput" type="hidden" name="year" value="2014">
	<button id="fourBtn">2014</button>
</form>


{{#each rank}}

<input id="position" type="hidden" name="position" value="{{this.Position}}">
<h1>{{this.Position}}</h1>
    <h1>{{this.Rank}}. {{this.Player}}</h1>

{{/each}}

